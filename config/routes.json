[
  {
    "path": "/",
    "page": "../app/pages/HomePage",
    "chunk": "main"
  },
  {
    "path": "/posts",
    "page": "../app/pages/PostsPage",
    "children": [
      {
        "path": "/:id",
        "page": "../app/pages/Post"
      }
    ]
  },
  {
    "path": "/projects",
    "page": "../app/pages/ProjectsPage"
  },
  {
    "path": "/resume",
    "page": "../app/pages/ResumePage"
  },
  {
    "path": "*",
    "page": "../app/pages/ErrorPage",
    "chunk": "error"
  }
]
